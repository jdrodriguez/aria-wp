{
  "name": "aria-wordpress-plugin",
  "version": "1.0.0",
  "description": "Aria - Your Website's Voice. AI-powered conversational assistant for WordPress.",
  "private": true,
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "zip": "npm run build && node scripts/create-zip.js"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "ai",
    "chatbot",
    "contact-form"
  ],
  "author": "Aria Development Team",
  "license": "Proprietary",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.27.1",
    "@playwright/test": "^1.54.1",
    "@wordpress/eslint-plugin": "^17.0.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "playwright": "^1.54.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.2.0",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.26.0",
    "@wordpress/components": "^29.12.0",
    "@wordpress/data": "^10.26.0",
    "@wordpress/element": "^6.26.0",
    "@wordpress/i18n": "^5.26.0",
    "@wordpress/icons": "^10.26.0",
    "prop-types": "^15.8.1"
  }
}
